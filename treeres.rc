#include <windows.h>
#include "treeres.h"

/////////////////////////////////////////////////////////////////////////////
// Menu
TREEMENU MENU DISCARDABLE 
BEGIN
	POPUP "&File"
	BEGIN
		MENUITEM "&New\tEnter", IDM_TREE_NEW
		MENUITEM "&Open\tF2", IDM_TREE_OPEN
		MENUITEM "&Save\tF3", IDM_TREE_SAVE
		MENUITEM SEPARATOR
		MENUITEM "E&xit\tESC", IDM_APP_EXIT
	END
	POPUP "&Edit"
	BEGIN
		MENUITEM "&Add a node\tIns", IDM_EDIT_ADD
		MENUITEM "&Delete a node\tDel", IDM_EDIT_DEL
		MENUITEM "&Modify a node\tF4", IDM_EDIT_MODIFY
		MENUITEM "&Search a node\tF5", IDM_EDIT_SEARCH
	END
	POPUP "&Timer"
	BEGIN
		MENUITEM "&Start", IDM_TIMER_ON
		MENUITEM "S&top", IDM_TIMER_OFF
	END
	POPUP "&Help"
	BEGIN
		MENUITEM "&Help...\tF1", IDM_APP_HELP
		MENUITEM "&About MenuDemo...", IDM_APP_ABOUT
	END
END
