// INPUT_DIALOG
IDD_INPUT_DIALOG DIALOGEX 0, 0, 200, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Enter a String"
BEGIN
    EDITTEXT        IDC_INPUT_EDIT, 10, 10, 180, 16, ES_AUTOHSCROLL|WS_TABSTOP|WS_GROUP
    DEFPUSHBUTTON   "Comfirm", IDOK, 70, 60, 60, 18
    PUSHBUTTON      "Cancel", IDCANCEL, 140, 60, 50, 18
END

